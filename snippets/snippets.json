{
  "D3_Skeleton": {
    "prefix": "d3-skeleton.lineChart",
    "body": [
      "var svg = d3.select('svg'),",
      "margin = {top: 20, right: 20, bottom: 30, left: 50},",
      "width = +svg.attr('width') - margin.left - margin.right,",
      "height = +svg.attr('height') - margin.top - margin.bottom,",
      "g = svg.append('g').attr('transform', 'translate(' + margin.left + ',' + margin.top + ')');",
      "",
      "var parseTime = d3.timeParse('%d-%b-%y');",
      "",
      "var x = d3.scaleTime()",
        "\t.rangeRound([0, width]);",
      "",
      "var y = d3.scaleLinear()",
        "\t.rangeRound([height, 0]);",
      "",
      "var line = d3.line()",
        "\t.x(function(d) { return x(d.date); })",
        "\t.y(function(d) { return y(d.close); });",
      "",
      "d3.tsv('data.tsv', function(d) {",
        "\td.date = parseTime(d.date);",
        "\td.close = +d.close;",
        "\treturn d;",
      "}, function(error, data) {",
        "\tif (error) throw error;",
      "",
        "x.domain(d3.extent(data, function(d) { return d.date; }));",
        "y.domain(d3.extent(data, function(d) { return d.close; }));",
      "",
        "\tg.append('g')",
            "\t\t\t.attr('transform', 'translate(0,' + height + ')')",
            "\t\t\t.call(d3.axisBottom(x))",
          "\t\t.select('.domain')",
            "\t\t\t.remove();",
      "",
        "\tg.append('g')",
            "\t\t\t.call(d3.axisLeft(y))",
          "\t\t.append('text')",
            "\t\t\t.attr('fill', '#000')",
            "\t\t\t.attr('transform', 'rotate(-90)')",
            "\t\t\t.attr('y', 6)",
            "\t\t\t.attr('dy', '0.71em')",
            "\t\t\t.attr('text-anchor', 'end')",
            "\t\t\t.text('Price ($)');",
      "",
        "\tg.append('path')",
            "\t\t.datum(data)",
            "\t\t.attr('fill', 'none')",
            "\t\t.attr('stroke', 'steelblue')",
            "\t\t.attr('stroke-linejoin', 'round')",
            "\t\t.attr('stroke-linecap', 'round')",
            "\t\t.attr('stroke-width', 1.5)",
            "\t\t.attr('d', line);",
      "});"
    ]
  }
}